<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/thirdparty_gold_quote_navigation"
    app:startDestination="@id/goldQuoteFragment3">

    <fragment
        android:id="@+id/goldQuoteFragment3"
        android:name="com.example.claimchapchap.insurancePolicies.thirdParty.quotes.GoldQuoteFragment"
        android:label="fragment_thirdparty_gold_quote"
        tools:layout="@layout/fragment_thirdparty_gold_quote" >
        <action
            android:id="@+id/action_goldQuoteFragment3_to_goldPaymentFragment3"
            app:destination="@id/goldPaymentFragment3" />
        <action
            android:id="@+id/action_goldQuoteFragment3_to_thirdPartyCoverActivity3"
            app:destination="@id/thirdPartyCoverActivity3" />
    </fragment>
    <fragment
        android:id="@+id/goldPaymentFragment3"
        android:name="com.example.claimchapchap.insurancePolicies.thirdParty.payment.GoldPaymentFragment"
        android:label="fragment_thirdparty_gold_payment"
        tools:layout="@layout/fragment_thirdparty_gold_payment" >
        <action
            android:id="@+id/action_goldPaymentFragment3_to_goldCardPayment"
            app:destination="@id/goldCardPayment" />
        <action
            android:id="@+id/action_goldPaymentFragment3_to_goldMobilePayment"
            app:destination="@id/goldMobilePayment" />
        <action
            android:id="@+id/action_goldPaymentFragment3_to_goldQuoteFragment3"
            app:destination="@id/goldQuoteFragment3" />
    </fragment>
    <fragment
        android:id="@+id/goldCardPayment"
        android:name="com.example.claimchapchap.insurancePolicies.thirdParty.payment.GoldCardPayment"
        android:label="fragment_thirdparty_gold_card_payment"
        tools:layout="@layout/fragment_thirdparty_gold_card_payment" >
        <action
            android:id="@+id/action_goldCardPayment_to_goldPaymentFragment3"
            app:destination="@id/goldPaymentFragment3" />
    </fragment>
    <fragment
        android:id="@+id/goldMobilePayment"
        android:name="com.example.claimchapchap.insurancePolicies.thirdParty.payment.GoldMobilePayment"
        android:label="fragment_thirdparty_gold_mobile_payment"
        tools:layout="@layout/fragment_thirdparty_gold_mobile_payment" >
        <action
            android:id="@+id/action_goldMobilePayment_to_goldPaymentFragment3"
            app:destination="@id/goldPaymentFragment3" />
    </fragment>
    <activity
        android:id="@+id/thirdPartyCoverActivity3"
        android:name="com.example.claimchapchap.insurancePolicies.thirdParty.ThirdPartyCoverActivity"
        android:label="activity_thirdparty_cover"
        tools:layout="@layout/activity_thirdparty_cover" />
</navigation>